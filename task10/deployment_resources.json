{
  "lambda-basic-execution": {
    "policy_content": {
      "Statement": [
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents",
            "dynamodb:GetItem",
            "dynamodb:Query",
            "dynamodb:PutItem",
            "dynamodb:Batch*",
            "dynamodb:DeleteItem",
            "ssm:PutParameter",
            "ssm:GetParameter",
            "kms:Decrypt",
            "cognito-idp:*"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "resource_type": "iam_policy"
  },
  "Tables": {
    "resource_type": "dynamodb_table",
    "hash_key_name": "id",
    "hash_key_type": "N",
    "sort_key_name": "number",
    "sort_key_type": "N",
    "read_capacity": 1,
    "write_capacity": 1,
    "global_indexes": [],
    "autoscaling": []
  },
  "Reservations": {
    "resource_type": "dynamodb_table",
    "hash_key_name": "tableNumber",
    "hash_key_type": "N",
    "sort_key_name": "date",
    "sort_key_type": "S",
    "read_capacity": 1,
    "write_capacity": 1,
    "global_indexes": [],
    "autoscaling": []
  },
  "simple-booking-userpool": {
    "resource_type": "cognito_idp",
    "password_policy": {
      "minimum_length": 12,
      "require_uppercase": false,
      "require_symbols": true,
      "require_lowercase": true,
      "require_numbers": true
    },
    "auto_verified_attributes": [],
    "sms_configuration": {},
    "username_attributes": [
      "email"
    ],
    "custom_attributes": [
      {
        "name": "firstName",
        "type": "String"
      },
      {
        "name": "lastName",
        "type": "String"
      }
    ],
    "client": {
      "client_name": "cmtr-6e999703-app-client-test",
      "generate_secret": false,
      "explicit_auth_flows": [
        "ALLOW_USER_PASSWORD_AUTH",
        "ALLOW_ADMIN_USER_PASSWORD_AUTH",
        "ALLOW_CUSTOM_AUTH",
        "ALLOW_USER_SRP_AUTH",
        "ALLOW_REFRESH_TOKEN_AUTH"
      ]
    }
  },
  "api_handler_signup-role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution"
    ],
    "resource_type": "iam_role",
    "allowed_accounts": [
      "${account_id}"
    ]
  },
  "api_handler_signin-role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution"
    ],
    "resource_type": "iam_role",
    "allowed_accounts": [
      "${account_id}"
    ]
  },
  "api_handler_tables_get_all-role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution"
    ],
    "resource_type": "iam_role",
    "allowed_accounts": [
      "${account_id}"
    ]
  },
  "api_handler_tables_get_by_id-role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution"
    ],
    "resource_type": "iam_role",
    "allowed_accounts": [
      "${account_id}"
    ]
  },
  "api_handler_tables_post-role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution"
    ],
    "resource_type": "iam_role",
    "allowed_accounts": [
      "${account_id}"
    ]
  },
  "api_handler_reservations_post-role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution"
    ],
    "resource_type": "iam_role",
    "allowed_accounts": [
      "${account_id}"
    ]
  },
  "api_handler_reservations_get_all-role": {
    "predefined_policies": [],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution"
    ],
    "resource_type": "iam_role",
    "allowed_accounts": [
      "${account_id}"
    ]
  },
  "task10_api": {
    "resource_type": "api_gateway",
    "deploy_stage": "api",
    "authorizers": {
      "authorizer": {
        "type": "COGNITO_USER_POOLS",
        "identity_source": "method.request.header.Authorization",
        "user_pools": [
          "simple-booking-userpool"
        ],
        "ttl": 300
      }
    },
    "dependencies": [
      {
        "resource_name": "api_handler_signup",
        "resource_type": "lambda"
      },
      {
        "resource_name": "api_handler_signin",
        "resource_type": "lambda"
      },
      {
        "resource_name": "api_handler_tables_get_all",
        "resource_type": "lambda"
      },
      {
        "resource_name": "api_handler_tables_get_by_id",
        "resource_type": "lambda"
      },
      {
        "resource_name": "api_handler_tables_post",
        "resource_type": "lambda"
      },
      {
        "resource_name": "api_handler_reservations_post",
        "resource_type": "lambda"
      },
      {
        "resource_name": "api_handler_reservations_get_all",
        "resource_type": "lambda"
      }
    ],
    "resources": {
      "/signup": {
        "POST": {
          "authorization_type": "NONE",
          "integration_type": "lambda",
          "lambda_name": "api_handler_signup"
        }
      },
      "/signin": {
        "POST": {
          "authorization_type": "NONE",
          "integration_type": "lambda",
          "lambda_name": "api_handler_signin"
        }
      },
      "/tables": {
        "GET": {
          "authorization_type": "authorizer",
          "integration_type": "lambda",
          "lambda_name": "api_handler_tables_get_all"
        }
      },
      "/tables/{tableId}": {
        "GET": {
          "authorization_type": "authorizer",
          "integration_type": "lambda",
          "lambda_name": "api_handler_tables_get_by_id"
        }
      },
      "/tables": {
        "POST": {
          "authorization_type": "authorizer",
          "integration_type": "lambda",
          "lambda_name": "api_handler_tables_post"
        }
      },
      "/reservations": {
        "POST": {
          "authorization_type": "authorizer",
          "integration_type": "lambda",
          "lambda_name": "api_handler_reservations_post"
        }
      },
      "/reservations": {
        "GET": {
          "authorization_type": "authorizer",
          "integration_type": "lambda",
          "lambda_name": "api_handler_reservations_get_all"
        }
      }
    }
  }
}
